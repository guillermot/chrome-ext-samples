{
  "name": "me sample",
  "description": "me sample",
  "version": "1.0",
  "manifest_version": 2,
  "background" : {
      "scripts" : ["background.js"],
      "persistent" : true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions" : [
    "webRequest",
    "tabs",
    "webNavigation",
    "http://*/*",
    "<all_urls>"
  ]
}